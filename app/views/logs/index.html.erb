<h1>Logs</h1>

<%if @paginate%>
  <%=will_paginate( @logs, renderer: BootstrapPagination::Rails )%>
<%end%>

<table class="table table-hover table-bordered table-condensed">
  <thead>
    <tr>
      <th>User</th>
      <th>Message</th>
      <th>Date-time</th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.user %></td>
        <td><%= log.log_stri %></td>
        <td><%= log.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

